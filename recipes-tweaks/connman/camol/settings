[global]
# The Cambrionix ntp server pool is strictly for use with Cambrionix
# manufactured devices
Timeservers=0.cbrx.pool.ntp.org;1.cbrx.pool.ntp.org;2.cbrx.pool.ntp.org;3.cbrx.pool.ntp.org

# Enable wired by default on Cambrionix Linux devices
[Wired]
Enable=true

# Enable wireless by default on Cambrionix Linux devices
[WiFi]
Enable=true
